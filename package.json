{
	"name": "gpumgr",
	"version": "0.1.1-alpha",
	"description": "gpumgr is a GPU manager for Linux allowing easy command line/web interface management of GPU power limits, clocks, fan speeds and more. gpumgr is multi-gpultural spiritual successor of amdpwrman.",
	"author": {
		"name": "Forbidden Era (Jai B.)",
		"email": "jai@shaped.ca",
		"url": "https://github.com/Shaped/"
	},
	"private": "true",
	"repository": "github:Shaped/gpumgr",
	"license": "GPL-3.0-or-later",
	"readme": "README.md",
	"homepage": "https://github.com/Shaped/gpumgr",
	"bugs": "https://github.com/Shaped/gpumgr/issues",
	"keywords": [
		"amd",
		"amdgpu",
		"radeon",
		"nvidia",
		"geforce",
		"rtx",
		"gtx",
		"intel",
		"xe",
		"gpu",
		"gpu management",
		"linux",
		"fan",
		"fan control"
	],
	"main": "src/gpumgr.js",
	"bin": {
		"gpumgr": "src/gpumgr.js"
	},
	"pkg": {
		"targets": [
			"node16-linux-x64"
		],
		"assets": [
			"./src/**/*"
		],
		"outputPath": "bin"
	},
	"scripts": {
		"build": "./build.sh",
		"build-release": "./build.sh release"
	},
	"dependencies": {
		"express": "^4.17.1",
		"sass": "^1.45.0",
		"saxon-js": "^2.3.0",
		"ws": "^8.4.0",
		"xml2json": "^0.12.0"
	},
	"engines": {
		"node": ">= 17.0.0",
		"npm": ">= 8.0.0"
	},
	"browserslist": [
		"defaults",
		"not IE 11",
		"maintained node versions"
	],
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/parser": "^7.16.6",
		"@babel/preset-env": "^7.16.5",
		"@babel/preset-react": "^7.16.5"
	}
}
